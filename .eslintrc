{
    "extends": "@antfu/react",
    "rules": {
      "@typescript-eslint/no-unused-vars": "off",
      "no-empty-pattern" : "warn",
      "semi": [
        "warn",
        "always"
      ],
      "@typescript-eslint/semi": [
        "warn",
        "always"
      ],
      "import/namespace": 0,
      "no-use-before-define": "off",
      "@typescript-eslint/no-use-before-define": 0,
      "eslint-comments/no-unlimited-disable" : 1
    }
  }